{
  "name": "hyperstatic",
  "description": "It creates a static version of any website",
  "homepage": "https://documentup.com/hyperstatic/hyperstatic",
  "version": "0.0.0",
  "main": "index.js",
  "bin": {
    "hyperstatic": "bin/index.js"
  },
  "author": {
    "email": "josefrancisco.verdu@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hyperstatic/hyperstatic.git"
  },
  "bugs": {
    "url": "https://github.com/hyperstatic/hyperstatic/issues"
  },
  "keywords": [],
  "dependencies": {
    "acho": "~4.0.2",
    "acho-skin-cli": "~2.0.1",
    "aigle": "~1.12.0",
    "chalk": "~2.4.1",
    "check-file": "~1.0.1",
    "download": "~7.0.0",
    "fs-extra": "~6.0.1",
    "html-get": "~1.0.1",
    "html-urls": "~1.0.9",
    "humanize-url": "~1.0.1",
    "listr": "~0.14.1",
    "meow": "~5.0.0",
    "normalize-url": "~3.0.1",
    "puppeteer": "~1.5.0",
    "update-notifier": "~2.5.0"
  },
  "devDependencies": {
    "coveralls": "latest",
    "finepack": "latest",
    "git-authors-cli": "latest",
    "git-dirty": "latest",
    "husky": "latest",
    "lint-staged": "latest",
    "nyc": "latest",
    "prettier-standard": "latest",
    "standard": "latest",
    "standard-markdown": "latest"
  },
  "engines": {
    "node": ">= 8"
  },
  "scripts": {
    "clean": "rm -rf node_modules",
    "coveralls": "nyc report --reporter=text-lcov | coveralls",
    "lint": "standard-markdown && standard",
    "precommit": "lint-staged",
    "pretest": "npm run lint",
    "pretty": "prettier-standard index.js {core,test,bin,scripts}/**/*.js --single-quote --print-width 100",
    "test": "nyc mocha"
  },
  "preferGlobal": true,
  "license": "MIT",
  "lint-staged": {
    "package.json": [
      "finepack",
      "git add"
    ],
    "*.js": [
      "prettier-standard",
      "git add"
    ],
    "*.md": [
      "standard-markdown",
      "git add"
    ]
  }
}
